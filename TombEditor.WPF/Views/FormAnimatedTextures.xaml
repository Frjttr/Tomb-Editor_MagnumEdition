<Window x:Class="TombEditor.WPF.Forms.FormAnimatedTextures"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:darkCtrl="clr-namespace:DarkUI.WPF.CustomControls;assembly=DarkUI.WPF"
        xmlns:darkUI="clr-namespace:DarkUI.WPF;assembly=DarkUI.WPF"
        xmlns:sg="clr-namespace:SpacedGridControl;assembly=SpacedGridControl"
        xmlns:local="clr-namespace:TombEditor.WPF.Forms"
        mc:Ignorable="d"
        Style="{DynamicResource CustomWindowStyle}"
        Title="Animated textures" Height="450" Width="800">
    <sg:SpacedGrid Margin="{x:Static darkUI:Defaults.MediumThickness}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="400" />
            <ColumnDefinition Width="400" />
        </Grid.ColumnDefinitions>
    </sg:SpacedGrid>
</Window>

